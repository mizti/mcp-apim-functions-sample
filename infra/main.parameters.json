{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "apimPublisherEmail": {
      "value": "${APIM_PUBLISHER_EMAIL}"
    },
    "apimPublisherName": {
      "value": "${APIM_PUBLISHER_NAME}"
    },
    "apimSkuName": {
      "value": "StandardV2"
    },
    "apimSkuCapacity": {
      "value": 1
    },
    "functionRuntimeName": {
      "value": "python"
    },
    "functionRuntimeVersion": {
      "value": "3.11"
    },
    "functionMaximumInstanceCount": {
      "value": 100
    },
    "functionInstanceMemoryMB": {
      "value": 2048
    }
  }
}
